<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ot.foodstorage.ui.MainSceneController">
   <children>
      <HBox layoutX="252.0" layoutY="48.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="126.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#addNewFood" prefHeight="53.0" prefWidth="126.0" text="Lisää raaka-aine" />
               </children>
            </VBox>
            <TableView fx:id="tableview" prefHeight="400.0" prefWidth="475.0">
              <columns>
                <TableColumn prefWidth="100.0" text="Nimi">
                    <cellValueFactory>
                        <PropertyValueFactory property="name" />
                    </cellValueFactory>
                </TableColumn>
                  <TableColumn prefWidth="113.0" text="Valmistaja">
                      <cellValueFactory>
                          <PropertyValueFactory property="manufacturer" />
                      </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="90.0" text="Eräpäivä">
                      <cellValueFactory>
                          <PropertyValueFactory property="dueDate" />
                      </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="82.0" text="Paino">
                      <cellValueFactory>
                          <PropertyValueFactory property="weight" />
                      </cellValueFactory>
                  </TableColumn>
                  <TableColumn prefWidth="87.0" text="Säilytys">
                      <cellValueFactory>
                          <PropertyValueFactory property="preservation" />
                      </cellValueFactory>
                  </TableColumn>
              </columns>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
